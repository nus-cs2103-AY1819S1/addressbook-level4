    <recipes>
        <name>Creamy Garlic Penne Pasta</name>
        <difficulty>3</difficulty>
        <cooktime>15M</cooktime>
        <instructions>
            <instruction>Melt butter and add garlic in a medium sauce pan.</instruction>
            <ingredients>
                <name>butter</name>
                <amount>2.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>garlic</name>
                <amount>2.0</amount>
                <unit>clove</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Cook over medium for 1 minute.</instruction>
            <cooktime>1M</cooktime>
        </instructions>
        <instructions>
            <instruction>Add flour and cook 1 minute, stirring constantly.</instruction>
            <cooktime>1M</cooktime>
            <ingredients>
                <name>flour</name>
                <amount>2.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Stir in broth and milk and cook, stirring frequently, until sauce boils and thickens.</instruction>
            <ingredients>
                <name>chicken broth</name>
                <amount>0.75</amount>
                <unit>cup</unit>
            </ingredients>
            <ingredients>
                <name>milk</name>
                <amount>0.75</amount>
                <unit>cup</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Add parsley, salt, pepper and cheese.</instruction>
            <ingredients>
                <name>parsley</name>
                <amount>2.0</amount>
                <unit>teaspoon</unit>
            </ingredients>
            <ingredients>
                <name>pepper</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
            <ingredients>
                <name>cheese</name>
                <amount>0.33</amount>
                <unit>cup</unit>
            </ingredients>
            <ingredients>
                <name>salt</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Stir until cheese is melted.</instruction>
        </instructions>
        <instructions>
            <instruction>Toss hot pasta with sauce and serve immediately.</instruction>
            <ingredients>
                <name>penne</name>
                <amount>150.0</amount>
                <unit>g</unit>
            </ingredients>
        </instructions>
