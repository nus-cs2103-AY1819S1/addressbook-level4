            <ingredients>
                <name>pepper</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
            <ingredients>
                <name>salt</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
            <ingredients>
                <name>dijon mustard</name>
                <amount>3.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>salmon</name>
                <amount>450.0</amount>
                <unit>g</unit>
            </ingredients>
