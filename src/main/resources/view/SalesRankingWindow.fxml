<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/sales.png" />
  </icons>
  <scene>
    <Scene>
      <VBox alignment="TOP_CENTER" prefHeight="250.0" prefWidth="310.0" spacing="10.0">
        <TableView fx:id="rankingTable" prefHeight="250.0" prefWidth="271.0">
          <columns>
            <TableColumn fx:id="rank" prefWidth="40" text="Rank" sortable="false"/>
            <TableColumn fx:id="dateOrItem" prefWidth="150" text="Key" sortable="false"/>
            <TableColumn fx:id="revenue" prefWidth="100" text="Revenue" sortable="false"/>
          </columns>
        </TableView>
      </VBox>
    </Scene>
  </scene>
</fx:root>
