<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <children>
         <Label fx:id="title" styleClass="moduleListPanel_title" text="Title" />
         <Label fx:id="subTitle" styleClass="moduleListPanel_subTitle" text="Subtitle" />
      </children>
   </HBox>
  <ListView fx:id="moduleListView" VBox.vgrow="ALWAYS" />
</VBox>
