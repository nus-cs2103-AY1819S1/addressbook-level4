<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER" prefHeight="72.0" prefWidth="340.0" VBox.vgrow="NEVER" styleClass="standard-background-color">
         <Label styleClass="label-bright-header" text="Match Test Completed!" />
   </HBox>
   <HBox alignment="CENTER" prefHeight="74.0" prefWidth="340.0" VBox.vgrow="NEVER" styleClass="standard-background-color">
      <children>
         <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="69.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label styleClass="label-bright" text="Topic: " />
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefHeight="69.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label fx:id="topicText" styleClass="label-bright" text="\$topicText" />
                  </children>
               </HBox>
            </children>
         </HBox>
         <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label styleClass="label-bright" text="No. of cards: " />
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label fx:id="numOfCardsText" styleClass="label-bright" text="\$numOfCardsText" />
                  </children>
               </HBox>
            </children>
         </HBox>
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="74.0" prefWidth="340.0" VBox.vgrow="NEVER" styleClass="standard-background-color">
      <children>
         <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label styleClass="label-bright" text="Test Date: " />
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label fx:id="testDateText" styleClass="label-bright" text="\$testDateText" />
                  </children>
               </HBox>
            </children>
         </HBox>
         <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
            <children>
               <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label styleClass="label-bright" text="Duration of Test: " />
                  </children>
               </HBox>
               <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                    <Label fx:id="durationText" styleClass="label-bright" text="\$durationText" />
                  </children>
               </HBox>
            </children>
         </HBox>
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" styleClass="standard-background-color">
      <children>
         <Label fx:id="numOfAttemptsText" style="-fx-font-size: 15pt" styleClass="label-bright" text="\$numOfAttemptsText" />
      </children>
   </HBox>
  <VBox fx:id="attemptListView" minWidth="340" prefWidth="340" styleClass="grid-pane" VBox.vgrow="ALWAYS">
     <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
     </padding>
     <children>
        <StackPane fx:id="attemptListPlaceholder" VBox.vgrow="ALWAYS" />
     </children>
  </VBox>
</VBox>
