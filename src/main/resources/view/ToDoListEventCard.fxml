<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns:fx="http://javafx.com/fxml/1" id="cardPaneToDo" fx:id="cardPaneToDo" prefHeight="25.0" prefWidth="400.0"
      styleClass="container" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8">
    <CheckBox mnemonicParsing="false" prefHeight="25.0" prefWidth="16.0" styleClass="button-bar"
              stylesheets="@DarkTheme.css" HBox.hgrow="ALWAYS"/>
    <Label fx:id="id" prefHeight="24.0" prefWidth="25.0" stylesheets="@DarkTheme.css" text="$id" HBox.hgrow="ALWAYS"/>
    <Label fx:id="title" prefHeight="24.0" prefWidth="175.0" stylesheets="@DarkTheme.css" text="$title"
           HBox.hgrow="ALWAYS"/>
    <Label fx:id="priority" prefHeight="22.0" prefWidth="100.0" stylesheets="@DarkTheme.css" text="$priority"
           HBox.hgrow="ALWAYS"/>
</HBox>
