<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121">
    <VBox alignment="CENTER_LEFT" spacing="16.0">
        <padding>
            <Insets top="16" right="16" bottom="16" left="16"/>
        </padding>
        <Text fx:id="medicineName" styleClass="medicine-details-title"/>
        <Text fx:id="medicineType" styleClass="medicine-details-text"/>
        <Text fx:id="effectiveDosage" styleClass="medicine-details-text"/>
        <Text fx:id="lethalDosage" styleClass="medicine-details-text"/>
        <Text fx:id="price" styleClass="medicine-details-text"/>
        <Text fx:id="quantity" styleClass="medicine-details-text"/>
    </VBox>
</StackPane>
