<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <VBox prefHeight="50.0" prefWidth="900.0" VBox.vgrow="NEVER">
        <children>
            <HBox maxHeight="70.0" prefHeight="70.0" VBox.vgrow="NEVER">
                <children>
                    <Text fx:id="commandText" fill="WHITE" boundsType="LOGICAL_VERTICAL_CENTER" HBox.hgrow="ALWAYS" text="Command: ">
                        <HBox.margin>
                            <Insets bottom="25.0" left="25.0" right="10.0" top="25.0"/>
                        </HBox.margin>
                    </Text>
                    <TextField fx:id="commandTextField" HBox.hgrow="ALWAYS" onAction="#handleCommandEntered" onKeyPressed="#handleKeyPress" promptText="Enter command here...">
                        <HBox.margin>
                            <Insets bottom="25.0" left="10.0" right="25.0" top="20.0"/>
                        </HBox.margin>
                    </TextField>
                </children>
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
            </HBox>
        </children>
    </VBox>
</StackPane>

